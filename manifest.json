{
	"name": "TTCV2",
	"author": "Kamarov",
	"version": "2024.01.21",
	"manifest_version": 2,
	"description": "Classic TankTrouble is alive once again!",
  
	"permissions": [
		"declarativeNetRequest",
		"declarativeNetRequestFeedback",
		"declarativeNetRequestWithHostAccess",
		"storage",
		"activeTab",
		"*://*.tanktrouble.com/",
		"*://*.raw.githubusercontent.com/"
	],

	"icons": {
		"16": "icon/16.png",
		"48": "icon/48.png",
		"128": "icon/128.png",
		"256": "icon/256.png"
	},

	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "icon/16.png",
			"48": "icon/48.png",
			"128": "icon/128.png"
		}
	}
}
